package com.cbt.entity;

import java.io.Serializable;

public class FactoryUserRelation implements Serializable {

	
	private Integer id;          
	private String factoryId;    //工厂ID
	private String userid;       //用户ID
	private String factoryUserRemark; //工厂添加客户备注
	
	
	
	public String getFactoryUserRemark() {
		return factoryUserRemark;
	}
	public void setFactoryUserRemark(String factoryUserRemark) {
		this.factoryUserRemark = factoryUserRemark;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getFactoryId() {
		return factoryId;
	}
	public void setFactoryId(String factoryId) {
		this.factoryId = factoryId;
	}
	public String getUserid() {
		return userid;
	}
	public void setUserid(String userid) {
		this.userid = userid;
	}
	@Override
	public String toString() {
		return "FactoryUserRelation [id=" + id + ", factoryId=" + factoryId
				+ ", userid=" + userid + ", factoryUserRemark="
				+ factoryUserRemark + "]";
	}
	
	
}
