/* Reset */
.ui-step:after {
    display: block;
    content: "";
    height: 0;
    font-size: 0;
    clear: both;
    overflow: hidden;
    visibility: hidden;
}
.ui-step {
    padding: 0 60px;
    position: relative;
}
ol {
    list-style: none outside none;
    margin: 0;
    padding: 0;
}
.ui-step-3 li {
    width: 50%;
}
.ui-step li {
    float: left;
    position: relative;
}
.step-done .ui-step-line {
    background-color:#05ce77;
}
.ui-step-line {
    height: 10px;
    background-color: #e0e0e0;
    box-shadow: inset 0 1px 1px rgba(0,0,0,.2);
    margin-top:30px;
}
.ui-step-cont {
    width: 103px;
    position: absolute;
    top: 0;
    left: -15px;
    text-align: center;
}
.step-done .ui-step-cont-number {
    background-color: #05ce77;
}
.ui-step-cont-number {
    display: inline-block;
    position: absolute;
    left: 0;
    top: 16px;
    width: 48px;
    height: 31px;
    line-height: 35px;
    border-radius: 50%;
    border-left: 2px solid #fff;
    border-right: 2px solid #fff;
    border: 2px solid rgb(214, 214, 214);
    font-family: tahoma;
	z-index:999;
    font-size: 16px;
    background-color: #b9b9b9;
     box-shadow: inset 1px 1px 2px rgba(0,0,0,.2);
}
.step-done .ui-step-cont-text {
    color: #28b779;
}
.ui-step-cont-text {
    position: relative;
    top:54px;
    left: -24px;
    font-size: 12px;
}
.step-active .ui-step-line {
    background-color: #e0e0e0;
}
.step-active .ui-step-cont-number {
    background-color:#28b779;

}
.ui-step-cont .ui-step-cont-number{color: #fff;}
.ui-step .step-end {
    width: 120px;
    position: absolute;
    top: 0;
    right: -60px;
}
.step-end .ui-step-line {
    display: none;
}
.step-active .ui-step-cont-text {
    color:#28b779;
    font-weight: bold;
}
