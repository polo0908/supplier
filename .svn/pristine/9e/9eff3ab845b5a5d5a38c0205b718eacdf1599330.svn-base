package com.cbt.entity;


/**
* @ClassName: ClientOrder 
* @Description: 客户历史订单
 */
public class ReOrderQuery extends ReOrder{

	private String beginDate;//查询开始时间
	private String endDate; //查询结束时间
    private Integer start;  //开始页
    private Integer pageSize; //每页显示数
	
    
	public Integer getStart() {
		return start;
	}

	public void setStart(Integer start) {
		this.start = start;
	}

	public Integer getPageSize() {
		return pageSize;
	}

	public void setPageSize(Integer pageSize) {
		this.pageSize = pageSize;
	}

	public String getBeginDate() {
		return beginDate;
	}

	public void setBeginDate(String beginDate) {
		this.beginDate = beginDate;
	}

	public String getEndDate() {
		return endDate;
	}

	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}

	@Override
	public String toString() {
		return "ReOrderQuery [beginDate=" + beginDate + ", endDate=" + endDate
				+ ", start=" + start + ", pageSize=" + pageSize
				+ ", getFactoryId()=" + getFactoryId() + ", getSaleCustomer()="
				+ getSaleCustomer() + ", getSalesId()=" + getSalesId()
				+ ", getFollowTime()=" + getFollowTime() + ", getHistoryId()="
				+ getHistoryId() + ", getFollowComment()=" + getFollowComment()
				+ ", getUser()=" + getUser() + ", getUserName()="
				+ getUserName() + ", getFollowUp()=" + getFollowUp()
				+ ", getId()=" + getId() + ", getUserid()=" + getUserid()
				+ ", getRequiredTime()=" + getRequiredTime()
				+ ", getComments()=" + getComments() + ", getReOrderState()="
				+ getReOrderState() + ", getTotalAmount()=" + getTotalAmount()
				+ ", getCreateTime()=" + getCreateTime() + ", toString()="
				+ super.toString() + ", getClass()=" + getClass()
				+ ", hashCode()=" + hashCode() + "]";
	}








  
	
   
	
	
	
	

	
	
}
