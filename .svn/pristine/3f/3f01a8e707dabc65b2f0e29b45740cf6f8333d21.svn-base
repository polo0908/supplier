package com.cbt.entity;

import java.io.Serializable;

public class FactoryMessage implements Serializable {

	
	
	 private Integer id;
	 private Integer orderMessageId;
	 private String  factoryMessageSendTime;  //回复消息时间 (包括客户追加回复时间)
	 private String  factoryMessageDetails;   //消息详情 (包括客户追加回复)
	 private Integer readStatus;             //未读：0   已读：1
	 private String  messageReadTime;        //消息阅读时间
	 private String  salesId;                //回复人ID
	 
	 
	 
	 
	public String getSalesId() {
		return salesId;
	}
	public void setSalesId(String salesId) {
		this.salesId = salesId;
	}
	public Integer getReadStatus() {
		return readStatus;
	}
	public void setReadStatus(Integer readStatus) {
		this.readStatus = readStatus;
	}
	public String getMessageReadTime() {
		return messageReadTime;
	}
	public void setMessageReadTime(String messageReadTime) {
		this.messageReadTime = messageReadTime;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getOrderMessageId() {
		return orderMessageId;
	}
	public void setOrderMessageId(Integer orderMessageId) {
		this.orderMessageId = orderMessageId;
	}
	public String getFactoryMessageSendTime() {
		return factoryMessageSendTime;
	}
	public void setFactoryMessageSendTime(String factoryMessageSendTime) {
		this.factoryMessageSendTime = factoryMessageSendTime;
	}
	public String getFactoryMessageDetails() {
		return factoryMessageDetails;
	}
	public void setFactoryMessageDetails(String factoryMessageDetails) {
		this.factoryMessageDetails = factoryMessageDetails;
	}
	@Override
	public String toString() {
		return "FactoryMessage [id=" + id + ", orderMessageId="
				+ orderMessageId + ", factoryMessageSendTime="
				+ factoryMessageSendTime + ", factoryMessageDetails="
				+ factoryMessageDetails + ", readStatus=" + readStatus
				+ ", messageReadTime=" + messageReadTime + ", salesId="
				+ salesId + "]";
	}
	 
	 
	 
	 
}
